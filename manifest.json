{
    "manifest_version": 3,
    "name": "Secure Password Generator",
    "version": "1.0",
    "description": "Generate strong, random passwords with one click.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    },
    "permissions": ["clipboardWrite"],
    "host_permissions": []
  }
  